{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex, nofollow"
        }
      ],
      "has": [
        {
          "type": "host",
          "value": "leo-deploy-mobile-git-main-leo-nobodys-projects.vercel.app"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://yourdomain.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "leo-deploy-mobile-git-main-leo-nobodys-projects.vercel.app"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/shouji/:path*",
      "destination": "https://tool.bitefu.net/shouji/:path*"
    },
    {
      "source": "/api/:path*",
      "destination": "http://47.114.174.121:8080/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
